programa {
    logico senhaValida
    caracter nomeUsuario
    caracter senha
    
	funcao inicio("algoritmo senha/=usuario") {
            escreva ("Insira seu nome de usu치rio: ")
	        leia (nomeUsuario)
            faca
            {
	        escreva ("Insira sua senha: ")
	        leia (senha)
	        
	        se (senha == nomeUsuario){
	        escreva ("\n ERRO! Insira uma senha v치lida diferente do seu nome de usu치rio." + "\n")
	        } 
	        senao {
	            escreva("Cadastro completo! Tenha um 칩timo dia :)")
	            senhaValida = verdadeiro
	        }
            } enquanto (senhaValida == falso)
	        }
	}